{
    "globals": [
        {
            "src": "node_modules/promise-polyfill/dist/polyfill.js",
            "name": "promise-polyfill.system.js"
        },

        {
            "src": "node_modules/regenerator-runtime/runtime.js",
            "name": "regenerator-runtime.system.js"
        },

        "node_modules/systemjs/dist/system.js",
        "node_modules/vue/dist/vue.js",
        "node_modules/pjax/pjax.js"
    ],
    "babel": {
        "system": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "ie": 11
                        }
                    }
                ]
            ]
        },
        "esm": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "esmodules": true
                        },
                        "modules": false
                    }
                ]
            ]
        }
    }
}
